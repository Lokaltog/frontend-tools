{
  "extends": ["airbnb", "airbnb/hooks", "plugin:prettier/recommended", "plugin:cypress/recommended"],
  "env": {
    "browser": false,
    "es6": true,
    "cypress/globals": true
  },
  "plugins": ["prettier", "cypress"],
  "parserOptions": {
    "sourceType": "module"
  },
  "rules": {
    "prettier/prettier": "error",
    "no-prototype-builtins": 0,
    "no-param-reassign": 0
  }
}
